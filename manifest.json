{
   "background": {
      "scripts": ["core/background.js"]
   },
   "content_scripts": [ {
      "css": [ "css/main.css" ],
      "js": [ "core/jquery-2.2.4.min.js", "core/foreground.js" ],
      "matches": [ "http://*.vk.com/*", "https://*.vk.com/*" ],
      "run_at": "document_end"
   } ],
   "browser_action": {
      "default_icon": "img/128.png",
      "default_title": "__MSG_title__"
   },
   "default_locale": "en_US",
   "description": "__MSG_desc__",
   "icons": {
      "128": "img/128.png",
      "16": "img/128.png",
      "48": "img/128.png"
   },
   "manifest_version": 2,
   "name": "__MSG_name__",
   "short_name": "__MSG_shortname__",
   "permissions": [ "webRequestBlocking", "webRequest", "storage", "cookies", "webNavigation", "tabs", "http://*/*", "https://*/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2018.07.16.4",
   "web_accessible_resources": [ "img/loading.gif"]
}
